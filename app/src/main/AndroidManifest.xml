<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.puzzleleaf.nanum">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".Splash" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AdminNumber" android:screenOrientation="portrait" android:windowSoftInputMode="adjustResize" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".AdminMenu" android:screenOrientation="portrait" android:windowSoftInputMode="adjustResize" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".AdminWrite" android:screenOrientation="portrait" android:windowSoftInputMode="adjustResize" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".AdminEdit" android:screenOrientation="portrait" android:windowSoftInputMode="adjustResize" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".MusicSelect" android:screenOrientation="portrait" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".MusicPlayer" android:screenOrientation="portrait" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".AdminRemove" android:screenOrientation="portrait" android:windowSoftInputMode="adjustResize" android:theme="@style/Theme.AppCompat.NoActionBar"/>
    </application>
</manifest>